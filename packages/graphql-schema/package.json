    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/logform": "^1.10.1",
    "@types/node-fetch": "^2.5.4",
    "@types/tempy": "^0.3.0",
    "apollo-server": "^2.9.12",
    "chokidar": "^3.3.0",
