    "winston": "^3.2.1"
  },
  "dependencies": {
    "date-fns": "^2.8.1",
    "graphql-type-json": "^0.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "typescript-memoize": "^1.0.0-alpha.3",
    "yn": "^3.1.1"
