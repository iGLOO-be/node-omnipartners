{
  "name": "@igloo-be-omnipartners/graphql-demo",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "yarn gen:types && gatsby build",
    "start": "yarn gen:types && gatsby develop -H 0.0.0.0",
    "gen:types": "apollo client:codegen --target typescript --includes \"src/**/*.{ts,tsx}\"",
    "test": "yarn gen:types && tsc --noEmit"
  },
  "dependencies": {
    "apollo": "^2.9.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-retry": "^2.2.13",
    "formik": "^1.5.4",
    "gatsby": "^2.4.2",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-typescript": "^2.0.13",
    "graphql-tag": "^2.10.1",
    "http-proxy-middleware": "^0.19.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "type-fest": "^0.4.1",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "eslint-config-react-app": "^4.0.0"
  }
}
